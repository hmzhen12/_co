/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=nsel);
    Nand(a=a, b=nsel, out=AnandNSel);
    Nand(a=b, b=sel, out=BnandSel);
    Nand(a=AnandNSel, b=BnandSel, out=out);
}
