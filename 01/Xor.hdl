/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=na);
    Nand(a=na, b=b, out=NAnandB);
    Not(in=b, out=nb);
    Nand(b=nb, a=a, out=AnandNB);
    Nand(a=NAnandB, b=AnandNB, out=out);
}
